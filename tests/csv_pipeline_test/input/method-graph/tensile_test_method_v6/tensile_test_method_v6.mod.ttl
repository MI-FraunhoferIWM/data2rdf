@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix emmo: <http://emmo.info/emmo#> .
@prefix holistic: <http://emmo.info/emmo/middle/holistic#> .
@prefix manufacturing: <http://emmo.info/emmo/middle/manufacturing#> .
@prefix mero: <http://emmo.info/emmo/top/mereotopology#> .
@prefix fileid: <http://abox-namespace-placeholder.org/> .
@prefix mt: <http://emmo.info/emmo/domain/mechanical-testing#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix semiotics: <http://emmo.info/emmo/middle/semiotics#> .

emmo:hasProperty a owl:ObjectProperty ;
    rdfs:label "has property" .

mt:hasInput a owl:ObjectProperty ;
    rdfs:label "has input" .

mt:hasOutput a owl:ObjectProperty ;
    rdfs:label "has output" .

holistic:hasParticipant a owl:ObjectProperty ;
    rdfs:label "has participant" .

semiotics:hasSign a owl:ObjectProperty ;
    rdfs:label "has sign" .

mero:hasPart a owl:ObjectProperty ;
    rdfs:label "has part" .

fileid:TensileTestSpecimen emmo:hasProperty fileid:ParallelLength ,
                                            fileid:SpecimenThickness,
                                            fileid:SpecimenType,
                                            fileid:SpecimenWidth ;
                           mero:hasPart fileid:Material .

fileid:TensileTestingMachine mt:hasOutput fileid:AbsoluteCrossheadTravel ;
                             semiotics:hasSign fileid:MachineData ;
                             mero:hasPart fileid:DisplacementTransducer,
                                          fileid:ForceMeasuringDevice .

fileid:Preload mt:hasInput fileid:TensileTestingMachine .

fileid:TestingFacility mero:hasPart fileid:Tester .

fileid:ForceMeasuringDevice mt:hasOutput fileid:StandardForce .

fileid:DisplacementTransducer mt:hasInput fileid:OriginalGaugeLength ;
                              mt:hasOutput fileid:Extension .

fileid:TestingRate mt:hasInput fileid:TensileTestingMachine .

fileid:TensileTestExperiment emmo:hasProperty fileid:ProjectName,
                                              fileid:ProjectNumber,
                                              fileid:Remark,
                                              fileid:Temperature,
                                              fileid:TestStandard,
                                              fileid:TimeStamp ;
                             mt:hasInput fileid:TensileTestSpecimen ;
                             holistic:hasParticipant fileid:TensileTestingMachine,
                                                     fileid:Tester,
                                                     fileid:TestingFacility .
